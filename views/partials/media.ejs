<!-- INSTAGRAM MEDIA UNDERNEATH -->
<div class="medias">
  <h2 class="medias__title">FOLLOW US ON INSTAGRAM</h2>
  <div class="medias__container">
    <% instagram.forEach(function(insta) { %>        
      <div class="column "><a href="<%= insta.permalink %>"><img src="<%= insta.media_url %>" alt="" class="medias__img"></a></div>
      <% }) %>
  </div>
</div>